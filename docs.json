{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Unpod Dev",
  "colors": {
    "primary": "#2563EB",
    "light": "#3B82F6",
    "dark": "#1D4ED8"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://unpod.dev"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "concepts"
            ]
          },
          {
            "group": "Core Components",
            "pages": [
              "bridge",
              "agent",
              "number",
              "provider"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/integration",
              "guides/authentication",
              "guides/webhooks"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api/overview",
              "api/authentication",
              "api/rate-limiting"
            ]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api/numbers",
              "api/calls",
              "api/messages",
              "api/webhooks"
            ]
          },
          {
            "group": "SDKs",
            "pages": [
              "sdk/javascript",
              "sdk/python",
              "sdk/java"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Status",
          "href": "https://status.unpod.dev",
          "icon": "signal"
