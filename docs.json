        {
          "anchor": "GitHub",
          "href": "https://github.com/parvbhullar",
          "icon": "github"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/7kQ4ewNSHZ",
          "icon": "message-circle"
        },
        {
          "anchor": "Changelog",
          "href": "https://unpod.dev/changelog",
          "icon": "list"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Documentation",
        "href": "https://docs.unpod.dev/"
      },
      {
        "label": "API Reference",
        "href": "https://docs.unpod.dev/api/overview"
