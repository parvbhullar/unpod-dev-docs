{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Unpod Dev",
  "colors": {
    "primary": "#796cff",
    "light": "#9a90ff",
    "dark": "#5a4fff"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/dark.svg",
    "dark": "/logo/light.svg",
    "href": "https://unpod.dev"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["introduction", "quickstart", "concepts"]
          },
          {
            "group": "Core Components",
            "pages": ["bridge", "agent", "number", "provider"]
          },
          {
            "group": "Guides",
            "pages": ["guides/integration", "guides/authentication", "guides/webhooks"]
          },
          {
            "group": "API Reference",
            "pages": ["api-reference/introduction"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "API Endpoints",
            "pages": ["api/overview", "api/authentication", "api/rate-limiting"]
          },
          {
            "group": "Resources",
            "pages": ["api/numbers", "api/calls", "api/messages", "api/webhooks"]
          },
          {
            "group": "SDKs",
            "pages": ["sdk/javascript", "sdk/python", "sdk/java"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "YouTube",
          "href": "https://www.youtube.com/@unpodai",
          "icon": "youtube"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/parvbhullar",
          "icon": "github"
        },
        {
          "anchor": "Discord",
          "href": "https://discord.gg/7kQ4ewNSHZ",
          "icon": "message-circle"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Documentation",
        "href": "https://docs.unpod.dev/"
      },
      {
        "label": "API Reference",
        "href": "https://docs.unpod.dev/api/overview"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://unpod.dev"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/parvbhullar",
      "twitter": "https://x.com/unpod_ai",
      "youtube": "https://www.youtube.com/@unpodai",
      "discord": "https://discord.gg/7kQ4ewNSHZ"
    }
  }
}
